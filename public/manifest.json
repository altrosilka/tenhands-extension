{
  "background": {
    "page": "background.html"
  },
  "description": "tetts",
  "icons": {
    "16": "icons/16.jpg",
    "48": "icons/48.jpg",
    "128": "icons/128.jpg"
  },
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "icons/48.jpg"
  },
  "manifest_version": 2,
  "homepage_url": "http://ya.ru",
  "name": "sadasd",
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "pages/createPost.html",
    "pages/afterInstall.html",
    "pack/page.css"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "css": [
        "pack/page.css"
      ]
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "pack/cabinetExecute.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.vk.com https://*.google.com; object-src 'self'",
  "version": "0.0.7"
}
